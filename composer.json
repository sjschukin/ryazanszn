{
    "name": "drupal/recommended-project",
    "description": "Project template for Drupal 8 projects with a relocated document root",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.2",
        "drupal/admin_toolbar": "^3.4",
        "drupal/antibot": "^2.0",
        "drupal/auditfiles": "^4.2",
        "drupal/bootstrap": "^3.23",
        "drupal/bootstrap_library": "^2.0",
        "drupal/captcha": "^2.0@beta",
        "drupal/chosen": "^5.0",
        "drupal/colorbox": "^2.0",
        "drupal/config_filter": "^2.4",
        "drupal/config_ignore": "^3.0@beta",
        "drupal/config_split": "^2.0@RC",
        "drupal/config_update": "^2.0@alpha",
        "drupal/contact_block": "^2.0",
        "drupal/contact_emails": "^1.19",
        "drupal/contact_storage": "^1.1",
        "drupal/core": "^10",
        "drupal/core-composer-scaffold": "^10",
        "drupal/core-project-message": "^9",
        "drupal/core-recommended": "^10",
        "drupal/image_widget_crop": "^3.0",
        "drupal/imce": "^3.0",
        "drupal/pathauto": "^1.8",
        "drupal/recaptcha": "^3.2",
        "drupal/recaptcha_v3": "^2.0@alpha",
        "drupal/recreate_block_content": "^3.0",
        "drupal/redirect": "^1.6",
        "drupal/site_settings": "^2.0@RC",
        "drupal/sitemap": "^2.0",
        "drupal/smtp": "^1.0",
        "drupal/snowball_stemmer": "^2.0",
        "drupal/token_filter": "^2.1",
        "drupal/transliterate_filenames": "^2.0",
        "drupal/views_bootstrap": "^3.3",
        "drupal/views_extras": "^3.0",
        "drupal/views_field_view": "^1.0@beta",
        "drupal/views_tree": "^3.0",
        "drupal/webform": "^6.2",
        "drupal/xmlsitemap": "^2.0",
        "drush/drush": "^12"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "web/"
            },
            "file-mapping": {
                "[web-root]/sites/development.services.yml": false
            }
        },
        "installer-paths": {
            "web/core": [
                "type:drupal-core"
            ],
            "web/libraries/{$name}": [
                "type:drupal-library"
            ],
            "web/modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "web/profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "web/themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "web/modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "web/themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/recommended-project template!          </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    },
    "require-dev": {
        "drupal/devel": "^5.3"
    }
}
